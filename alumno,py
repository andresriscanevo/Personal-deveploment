from xml.dom.minidom import Notation


class Alummno:
    nombre=""
    nota=0.0
    def __init__(self,nombre,nota):
        self.nombre=nombre
        self.nota=nota
    def definitiva(self):
        if self.nota>60:
            dato=(f"el estudiante de nombre {self.nombre} aprobado con una nota {self.nota}")
        else:
            dato=(f"el estudiante de nombre {self.nombre} a reprobado dado que obtuvo una nota de {self.nota}")
        return dato
estudiante1=Alummno("andres",25)
a=estudiante1.definitiva()
print(a)


